END_SCRIPT



[ENTITY villager: SCRIPT]

ON_TALK greeting()

IF flag THEN test() ELSE default()

BLOCK default {
    MOVE RANDINT(4)
    SAY $CRY
}

BLOCK test {

}

BLOCK greeting {
    SAY("Why hello there!")
}

END_SCRIPT

